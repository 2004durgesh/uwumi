    "@tamagui/animations-moti": "^1.129.12",
    "@tamagui/babel-plugin": "^1.129.12",
    "@tamagui/cli": "^1.129.12",
    "@tamagui/config": "^1.129.12",
    "@tamagui/lucide-icons": "^1.129.12",
    "@tamagui/portal": "^1.129.12",
    "@tanstack/react-query": "^5.66.0",
    "axios": "^1.7.9",
